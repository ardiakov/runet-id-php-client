{
    "name": "runet-id/api-client",
    "description": "The Official RUNET-ID API Client",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Valentin Udaltsov",
            "email": "udaltsov@ruvents.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "ruvents/http-client": "^2.1",
        "ruvents/data-reconstructor": "^1.0",
        "symfony/options-resolver": "^2.8||^3.0"
    },
    "autoload": {
        "psr-4": {
            "RunetId\\ApiClient\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
